      <Label layoutX="58.0" layoutY="43.0" text="ERROR: INVALID INPUT" textFill="#f50000">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label fx:id="errorMessage" layoutX="58.0" layoutY="92.0" prefHeight="17.0" prefWidth="285.0" text="Label">
